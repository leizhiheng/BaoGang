<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:android_custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <LinearLayout
        android:id="@+id/layout_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <FrameLayout
            android:layout_width="fill_parent"
            android:layout_height="@dimen/dp_166">
            <ImageView
                android:id="@+id/iv_main"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:scaleType="fitXY"
                android:src="@drawable/icon_dianpubanner"/>
            <LinearLayout
                android:id="@+id/layout_store_main"
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:orientation="vertical">

                <RelativeLayout
                    android:id="@+id/top"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_46">

                    <!-- title bar 左侧返回布局 -->

                    <LinearLayout
                        android:id="@+id/layout_back"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_46"
                            android:background="@null"
                            android:contentDescription="@null"
                            android:focusable="false"
                            android:padding="@dimen/dp_13"
                            android:scaleType="centerInside"
                            android:src="@drawable/ic_arrow_back" />
                    </LinearLayout>

                    <!-- title bar 中间标题布局 -->

                    <TextView
                        android:id="@+id/btn_search"
                        android:layout_width="@dimen/dp_250"
                        android:layout_height="@dimen/dp_40"
                        android:layout_centerInParent="true"
                        android:layout_margin="@dimen/dp_6"
                        android:background="@drawable/common_radius_white_5pd"
                        android:drawableLeft="@drawable/gray_search"
                        android:drawablePadding="@dimen/dp_3"
                        android:gravity="center_vertical"
                        android:hint="搜索店铺商品"
                        android:paddingLeft="@dimen/dp_10"
                        android:textColor="@color/gr_l_color"
                        android:textSize="@dimen/sp_15" />

                    <FrameLayout
                        android:id="@+id/layout_right"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginLeft="10dp">

                        <ImageView
                            android:layout_width="@dimen/dp_46"
                            android:layout_height="@dimen/dp_46"
                            android:background="@null"
                            android:contentDescription="@null"
                            android:padding="@dimen/dp_13"
                            android:scaleType="centerInside"
                            android:src="@drawable/icon_3point" />
                    </FrameLayout>
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/dp_120"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <com.common.view.imageview.CircleImageView
                            android:id="@+id/iv_store_icon"
                            android:layout_width="@dimen/dp_60"
                            android:layout_height="@dimen/dp_60"
                            android:layout_margin="@dimen/dp_10"
                            android:src="@drawable/ic_launcher" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/btn_go2store_details"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:orientation="horizontal"
                                android:paddingBottom="@dimen/dp_5"
                                android:paddingTop="@dimen/dp_10">

                                <TextView
                                    android:id="@+id/tv_store_name"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="七彩生活自营"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/commen_text_size_14" />

                                <ImageView
                                    android:layout_width="@dimen/dp_17"
                                    android:layout_height="@dimen/dp_17"
                                    android:layout_marginLeft="@dimen/dp_5"
                                    android:background="@drawable/common_radius_orange_red_3dp"
                                    android:padding="@dimen/dp_2"
                                    android:scaleType="centerInside"
                                    android:src="@drawable/icon_arraw_right_white" />
                            </LinearLayout>

                            <TextView
                                android:id="@+id/tv_attention_num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="5人关注"
                                android:textColor="@color/white"
                                android:textSize="@dimen/commen_text_size_12" />

                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:gravity="bottom"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/dp_10"
                            android:layout_marginRight="@dimen/dp_10"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:id="@+id/btn_attention"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_5"
                                android:gravity="center_horizontal"
                                android:orientation="vertical"
                                android:paddingRight="@dimen/dp_10">

                                <ImageView
                                    android:layout_width="@dimen/btn_size_product_detials"
                                    android:layout_height="@dimen/btn_size_product_detials"
                                    android:scaleType="centerInside"
                                    android:src="@drawable/icon_guanzhu" />

                                <TextView
                                    android:id="@+id/tv_attentin"
                                    android:layout_width="wrap_content"
                                    android:layout_height="fill_parent"
                                    android:layout_marginTop="@dimen/dp_5"
                                    android:gravity="center_vertical"
                                    android:maxLines="2"
                                    android:text="加关注"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/sp_14" />

                            </LinearLayout>

                            <View
                                android:layout_width="@dimen/dp_0.1"
                                android:layout_height="fill_parent"
                                android:layout_margin="@dimen/dp_5"
                                android:background="#D3D3D3" />

                            <LinearLayout
                                android:id="@+id/btn_share"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_5"
                                android:gravity="center_horizontal"
                                android:orientation="vertical"
                                android:paddingRight="@dimen/dp_10">

                                <ImageView
                                    android:layout_width="@dimen/btn_size_product_detials"
                                    android:layout_height="@dimen/btn_size_product_detials"
                                    android:scaleType="centerInside"
                                    android:src="@drawable/icon_share_white" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="fill_parent"
                                    android:layout_marginTop="@dimen/dp_5"
                                    android:gravity="center_vertical"
                                    android:maxLines="2"
                                    android:text="分享"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/sp_14" />

                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

        </FrameLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="@dimen/dp_50"
            android:orientation="horizontal"
            android:weightSum="3">

            <LinearLayout
                android:id="@+id/btn_allpro"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:background="@drawable/common_btn_gray_selector_with_nostroke"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_allpro_num"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:text="30"
                    android:textColor="@color/black"
                    android:textSize="@dimen/commen_text_size_16" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/sp_5"
                    android:gravity="center_vertical"
                    android:text="全部商品"
                    android:textColor="#9A9A9C"
                    android:textSize="@dimen/sp_10" />
            </LinearLayout>

            <View
                android:layout_width="@dimen/dp_0.1"
                android:layout_height="fill_parent"
                android:layout_marginBottom="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_10"
                android:background="#C1C2C3" />

            <LinearLayout
                android:id="@+id/btn_newpro"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:background="@drawable/common_btn_gray_selector_with_nostroke"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_newpro_num"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:text="5"
                    android:textColor="@color/black"
                    android:textSize="@dimen/commen_text_size_16" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/sp_5"
                    android:gravity="center_vertical"
                    android:text="新品"
                    android:textColor="#9A9A9C"
                    android:textSize="@dimen/sp_10" />
            </LinearLayout>

            <View
                android:layout_width="@dimen/dp_0.1"
                android:layout_height="fill_parent"
                android:layout_marginBottom="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_10"
                android:background="#C1C2C3" />

            <LinearLayout
                android:id="@+id/btn_discountpro"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:background="@drawable/common_btn_gray_selector_with_nostroke"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_discountpro_num"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:text="1"
                    android:textColor="@color/black"
                    android:textSize="@dimen/commen_text_size_16" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/sp_5"
                    android:gravity="center_vertical"
                    android:text="促销"
                    android:textColor="#9A9A9C"
                    android:textSize="@dimen/sp_10" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_bottom"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/dp_46"
        android:layout_alignParentBottom="true"
        android:orientation="vertical">

        <View
            android:layout_width="fill_parent"
            android:layout_height="@dimen/dp_0.1"
            android:background="#C1C2C3" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:weightSum="3">

            <LinearLayout

                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/btn_all_category"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/store_btn_selector_allpro"
                    android:drawablePadding="@dimen/dp_5"
                    android:gravity="center_vertical"
                    android:text="全部分类"
                    android:textColor="@drawable/store_text_selector"
                    android:textSize="@dimen/commen_text_size_14" />
            </LinearLayout>

            <View
                android:layout_width="@dimen/dp_0.1"
                android:layout_height="fill_parent"
                android:background="#C1C2C3" />

            <LinearLayout

                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/btn_store_detials"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/store_btn_selector_details"
                    android:drawablePadding="@dimen/dp_5"
                    android:gravity="center_vertical"
                    android:text="店铺主页"
                    android:textColor="@drawable/store_text_selector"
                    android:textSize="@dimen/commen_text_size_14" />
            </LinearLayout>

            <View
                android:layout_width="@dimen/dp_0.1"
                android:layout_height="fill_parent"
                android:background="#C1C2C3" />

            <LinearLayout

                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/btn_service"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/store_btn_selector_service"
                    android:drawablePadding="@dimen/dp_5"
                    android:gravity="center_vertical"
                    android:text="联系客服"
                    android:textColor="@drawable/store_text_selector"
                    android:textSize="@dimen/commen_text_size_14" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

    <com.common.view.scrollview.NestScrollView
        android:id="@+id/scrollview"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_above="@id/layout_bottom"
        android:layout_below="@id/layout_top">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical">

            <include
                layout="@layout/common_line_gray"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="@dimen/dp_80"
                android:orientation="horizontal"
                android:weightSum="4">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <com.common.view.circle.RingProgressView
                        android:id="@+id/ring"
                        android:layout_width="@dimen/dp_40"
                        android:layout_height="@dimen/dp_40"
                        android_custom:roundColor="#C2C2C3"
                        android_custom:roundProgressColor="#F03A1A"
                        android_custom:roundWidth="@dimen/dp_2"
                        android_custom:style="STROKE"
                        android_custom:max="100"
                        android_custom:textColor="#F03A1A"
                        android_custom:textIsDisplayable="true"
                        android_custom:textSize="@dimen/commen_text_size_14" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/dp_5"
                        android:text="综合评分"
                        android:textColor="#666666"
                        android:textSize="@dimen/commen_text_size_12" />
                </LinearLayout>

                <View
                    android:layout_width="@dimen/dp_0.1"
                    android:layout_height="fill_parent"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:layout_marginTop="@dimen/dp_15"
                    android:background="#C2C2C3" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_weight="3"
                    android:orientation="horizontal"
                    android:weightSum="4">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:gravity="center_horizontal"
                        android:orientation="vertical"
                        android:weightSum="3">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="描述相符"
                            android:textColor="#999999"
                            android:textSize="@dimen/commen_text_size_12" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="服务态度"
                            android:textColor="#999999"
                            android:textSize="@dimen/commen_text_size_12" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="发货速度"
                            android:textColor="#999999"
                            android:textSize="@dimen/commen_text_size_12" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:gravity="center_horizontal"
                        android:orientation="vertical"
                        android:weightSum="3">

                        <TextView
                            android:id="@+id/tv_describ_score"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="9.36分"
                            android:textColor="#ff521d"
                            android:textSize="@dimen/commen_text_size_12" />

                        <TextView
                            android:id="@+id/tv_service_score"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="9.36分"
                            android:textColor="#ff521d"
                            android:textSize="@dimen/commen_text_size_12" />

                        <TextView
                            android:id="@+id/tv_release_score"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="5.36分"
                            android:textColor="#10bc31"
                            android:textSize="@dimen/commen_text_size_12" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="fill_parent"
                        android:layout_weight="2"
                        android:gravity="center_horizontal"
                        android:orientation="vertical"
                        android:weightSum="3">

                        <TextView
                            android:id="@+id/tv_describ_percentate"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="高于同行60%"
                            android:textColor="#ff521d"
                            android:textSize="@dimen/commen_text_size_12" />

                        <TextView
                            android:id="@+id/tv_service_percentate"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="高于同行2.5%"
                            android:textColor="#ff521d"
                            android:textSize="@dimen/commen_text_size_12" />

                        <TextView
                            android:id="@+id/tv_release_percentate"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="低于同行5%"
                            android:textColor="#10bc31"
                            android:textSize="@dimen/commen_text_size_12" />
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>


            <include
                layout="@layout/common_line_gray"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_40"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="@dimen/dp_10"
                android:text="在线客服"
                android:textColor="#666666"
                android:textSize="@dimen/commen_text_size_12" />

            <include
                layout="@layout/common_line_gray"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" />

            <LinearLayout
                android:id="@+id/layout_company_name"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/dp_40"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginRight="@dimen/dp_10"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawablePadding="5dp"
                    android:gravity="center_vertical"
                    android:maxLines="1"
                    android:text="公司名称"
                    android:textColor="#9E9E9E"
                    android:textSize="@dimen/commen_text_size_12" />

                <TextView
                    android:id="@+id/tv_company_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_15"
                    android:drawablePadding="5dp"
                    android:gravity="center_vertical"
                    android:maxLines="1"
                    android:text="深圳英迈思科技文化有限公司"
                    android:textColor="@color/black"
                    android:textSize="@dimen/commen_text_size_12" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginRight="@dimen/dp_5"
                    android:layout_weight="1"
                    android:gravity="center_vertical|right"
                    android:maxLines="1"
                    android:text="查询执照"
                    android:textColor="#999999"
                    android:textSize="@dimen/sp_10" />

                <ImageView
                    android:layout_width="@dimen/dp_16"
                    android:layout_height="@dimen/dp_16"
                    android:scaleType="centerInside"
                    android:src="@drawable/icon_arrow_right_gray" />
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="0.01dp"
                android:layout_marginLeft="@dimen/dp_10"
                android:background="#D3D3D3" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="@dimen/dp_40"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginRight="@dimen/dp_10"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawablePadding="5dp"
                    android:gravity="center_vertical"
                    android:maxLines="1"
                    android:text="所在地区"
                    android:textColor="#9E9E9E"
                    android:textSize="@dimen/commen_text_size_12" />

                <TextView
                    android:id="@+id/tv_adress"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_15"
                    android:drawablePadding="5dp"
                    android:gravity="center_vertical"
                    android:maxLines="1"
                    android:text="广东 深圳 宝安区"
                    android:textColor="@color/black"
                    android:textSize="@dimen/commen_text_size_12" />
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="0.01dp"
                android:layout_marginLeft="@dimen/dp_10"
                android:background="#D3D3D3" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="@dimen/dp_40"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginRight="@dimen/dp_10"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawablePadding="5dp"
                    android:gravity="center_vertical"
                    android:maxLines="1"
                    android:text="详细地址"
                    android:textColor="#9E9E9E"
                    android:textSize="@dimen/commen_text_size_12" />

                <TextView
                    android:id="@+id/tv_details_adress"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_15"
                    android:layout_weight="1"
                    android:drawablePadding="5dp"
                    android:gravity="center_vertical"
                    android:maxLines="1"
                    android:text="西乡固戍航城研发大厦2楼"
                    android:textColor="@color/black"
                    android:textSize="@dimen/commen_text_size_12" />

                <LinearLayout
                    android:id="@+id/btn_location"
                    android:layout_width="@dimen/dp_40"
                    android:layout_height="@dimen/dp_40"
                    android:gravity="center_vertical|right">

                    <ImageView
                        android:layout_width="@dimen/dp_20"
                        android:layout_height="@dimen/dp_20"
                        android:scaleType="centerInside"
                        android:src="@drawable/location" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="0.01dp"
                android:layout_marginLeft="@dimen/dp_10"
                android:background="#D3D3D3" />
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="@dimen/dp_40"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginRight="@dimen/dp_10"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawablePadding="5dp"
                    android:gravity="center_vertical"
                    android:maxLines="1"
                    android:text="开店时间"
                    android:textColor="#9E9E9E"
                    android:textSize="@dimen/commen_text_size_12" />

                <TextView
                    android:id="@+id/tv_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_15"
                    android:layout_weight="1"
                    android:drawablePadding="5dp"
                    android:gravity="center_vertical"
                    android:maxLines="1"
                    android:text="2015-09-21"
                    android:textColor="@color/black"
                    android:textSize="@dimen/commen_text_size_12" />

            </LinearLayout>

            <include
                layout="@layout/common_line_gray"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" />

        </LinearLayout>
    </com.common.view.scrollview.NestScrollView>
