<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <com.baosteel.qcsh.ui.view.TitleBar
        android:id="@+id/title_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <!--
                 <fragment
                android:id="@+id/fragment_banner"
                android:name="com.baosteel.qcsh.ui.fragment.commen.BannerFragment"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_160"
                class="com.baosteel.qcsh.ui.fragment.commen.BannerFragment" />
            -->

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_160"
                android:scaleType="centerCrop"
                android:src="@drawable/ic_healthy_community" />

            <com.common.view.other.GridViewInScrollView
                android:id="@+id/gridview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:horizontalSpacing="@dimen/dp_15"
                android:numColumns="2"
                android:padding="@dimen/dp_15"
                android:stretchMode="columnWidth"
                android:verticalSpacing="@dimen/dp_15" />
        </LinearLayout>
    </ScrollView>

</LinearLayout>