<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_64"
        android:layout_margin="@dimen/dp_10" >

        <com.common.view.listview.HorizontialListView
            android:id="@+id/lv_h"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:visibility="visible" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/tv_delivery_gohome"
            android:layout_width="@dimen/dp_80"
            android:layout_height="@dimen/dp_32"
            android:layout_marginLeft="@dimen/dp_10"
            android:background="@drawable/delivery_select"
            android:gravity="center"
            android:text="送货上门"
            android:textColor="@color/g_orage_color"
            android:textSize="@dimen/sp_13" />

        <TextView
            android:id="@+id/tv_gohome_goget"
            android:layout_width="@dimen/dp_80"
            android:layout_height="@dimen/dp_32"
            android:layout_marginLeft="@dimen/dp_10"
            android:background="@drawable/delivery_noselect"
            android:gravity="center"
            android:text="上门自提"
            android:textColor="@color/gr_l_color"
            android:textSize="@dimen/sp_13" />
    </LinearLayout>

    <TextView
        android:id="@+id/tv_fare"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_10"
        android:text="商品运费5元"
        android:textColor="@color/gr_l_color"
        android:textSize="@dimen/sp_11" />

    <LinearLayout
        android:id="@+id/lin_tips"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/rel_mentionpoint"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45"
            android:background="@color/white" >

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0.5"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="@dimen/dp_10"
                android:background="@color/line" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:paddingLeft="@dimen/dp_10"
                android:text="自提点"
                android:textColor="@color/font_q_color"
                android:textSize="@dimen/sp_15" />

            <TextView
                android:id="@+id/tv_mentionpoint"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_80"
                android:text="食品超市牡丹江站"
                android:textColor="@color/font_color"
                android:textSize="@dimen/sp_15" />

            <ImageView
                android:layout_width="@dimen/dp_10"
                android:layout_height="@dimen/dp_16"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dp_10"
                android:src="@drawable/right_press"
                android:text="@string/text_business"
                android:textSize="@dimen/sp_14" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel_address"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45"
            android:background="@color/white" >

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0.5"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="@dimen/dp_10"
                android:background="@color/line" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:paddingLeft="@dimen/dp_10"
                android:text="地址"
                android:textColor="@color/font_q_color"
                android:textSize="@dimen/sp_15" />

            <TextView
                android:id="@+id/tv_address"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/img_po"
                android:layout_marginRight="@dimen/dp_10"
                android:maxLines="2"
                android:ellipsize="end"
                android:layout_marginLeft="@dimen/dp_80"
                android:text="上海宝山牡丹江路184号"
                android:textColor="@color/font_color"
                android:textSize="@dimen/sp_15" />

            <ImageView
                android:id="@+id/img_po"
                android:layout_width="@dimen/dp_21"
                android:layout_height="@dimen/dp_21"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dp_10"
                android:src="@drawable/location"
                android:text="@string/text_business"
                android:textSize="@dimen/sp_14" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel_phone"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45"
            android:background="@color/white" >

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0.5"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="@dimen/dp_10"
                android:background="@color/line" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:paddingLeft="@dimen/dp_10"
                android:text="电话"
                android:textColor="@color/font_q_color"
                android:textSize="@dimen/sp_15" />

            <TextView
                android:id="@+id/tv_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_80"
                android:text="021-1234567"
                android:textColor="@color/font_color"
                android:textSize="@dimen/sp_15" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel_date"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45"
            android:background="@color/white" >

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0.5"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="@dimen/dp_10"
                android:background="@color/line" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:paddingLeft="@dimen/dp_10"
                android:text="自提时间"
                android:textColor="@color/font_q_color"
                android:textSize="@dimen/sp_15" />

            <TextView
                android:id="@+id/tv_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_80"
                android:text="2015-06-07"
                android:textColor="@color/font_color"
                android:textSize="@dimen/sp_15" />

            <ImageView
                android:layout_width="@dimen/dp_21"
                android:layout_height="@dimen/dp_21"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dp_10"
                android:src="@drawable/peisongshijian"
                android:text="@string/text_business"
                android:textSize="@dimen/sp_14" />
        </RelativeLayout>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_0.5"
        android:background="@color/line" />

</LinearLayout>