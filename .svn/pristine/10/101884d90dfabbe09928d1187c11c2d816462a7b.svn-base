public class CashType {
    private String name;
    private String detail;

    public CashType(String name, String detail) {
        this.name = name;
        this.detail = detail;
    }

    public String getName() {
        return name;
    }

    public CashType setName(String name) {
        this.name = name;
        return this;
    }

    public String getDetail() {
        return detail;
    }

    public CashType setDetail(String detail) {
        this.detail = detail;
        return this;
    }
}
